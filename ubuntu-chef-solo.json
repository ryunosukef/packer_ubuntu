{
  "variables": {
    "aws_access_key": "",
    "aws_secret_key": ""
  },
  "builders": [{
    "type": "amazon-ebs",
    "access_key": "{{user `aws_access_key`}}",
    "secret_key": "{{user `aws_secret_key`}}",
    "region": "ap-northeast-1",
    "source_ami": "ami-b945ddb8",
    "instance_type": "t1.micro",
    "ssh_username": "ubuntu",
    "ssh_timeout": "5m",
    "ami_name": "packer-ubuntu-{{timestamp}}"
  }],
 "provisioners": [{
    "type": "chef-solo",
    "cookbook_paths": ["cookbooks"],
    "run_list": [],
    "prevent_sudo": false,
    "skip_install": false
  }]
}
